package com.unary;

public class OperatorEx {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
char ch='A';
System.out.println(ch);//
System.out.println(++ch);//
System.out.println(--ch);//
System.out.println((int)ch);

int x=10;
int y=x+1;
System.out.println(y);
System.out.println(x);
	
	
	}

}
